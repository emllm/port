
{
    "name": "pwa-marketplace",
    "version": "1.0.0",
    "description": "A secure marketplace for Progressive Web Applications",
    "main": "server/index.js",
    "scripts": {
      "start": "node server/index.js",
      "dev": "nodemon server/index.js",
      "build": "webpack --mode=production",
      "build:dev": "webpack --mode=development",
      "test": "jest",
      "test:watch": "jest --watch",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "docker:build": "docker build -f docker/Dockerfile.marketplace -t pwa-marketplace .",
      "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
      "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d"
    },
    "dependencies": {
      "express": "^4.18.2",
      "cors": "^2.8.5",
      "helmet": "^7.0.0",
      "compression": "^1.7.4",
      "express-rate-limit": "^6.8.1",
      "http-proxy-middleware": "^2.0.6",
      "ws": "^8.13.0",
      "uuid": "^9.0.0",
      "dotenv": "^16.3.1"
    },
    "devDependencies": {
      "nodemon": "^3.0.1",
      "webpack": "^5.88.2",
      "webpack-cli": "^5.1.4",
      "html-webpack-plugin": "^5.5.3",
      "css-loader": "^6.8.1",
      "style-loader": "^3.3.3",
      "mini-css-extract-plugin": "^2.7.6",
      "eslint": "^8.45.0",
      "jest": "^29.6.2",
      "@types/jest": "^29.5.3"
    },
    "keywords": [
      "pwa",
      "progressive-web-app",
      "marketplace",
      "sandbox",
      "docker",
      "github"
    ],
    "author": "PWA Marketplace Team",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/your-org/pwa-marketplace.git"
    },
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=8.0.0"
    }
  }
  